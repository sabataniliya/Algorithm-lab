#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n;
    printf( "size of array\n");
    scanf("%d\n",n);

     int a[n];
     printf("arrays elements\n");
     for(int i=0;i<=n;i++){
       scanf("%d\n",a[i]);
     }
     printf("unsorted array\n");
      for(int i=0;i<=n;i++){
       scanf("%d\n",a[i]);
     }

     insertion(a,n);

     return 0;
}

void insertion(int a[],int n ){

  int i,j;
  for(i=0;i<=n;i++){
    int temp=a[i];
    j=i-1;
    while(j<=0&&a[j]>temp){
        a[j+1]=j;
        j--;
    }
    a[j+1]=temp;
  }
    printf("sorted array\n");
      for(int i=0;i<=n;i++){
       scanf("%d\n",a[i]);
     }
  }


